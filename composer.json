{
    "name": "villaflor/taboola-sdk",
    "description": "This package contains the open source PHP SDK that allows you to access the Taboola Platform from you PHP application.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Villaflor\\TaboolaSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "authors": [
        {
            "name": "Mark Anthony Villaflor",
            "email": "villaflor@live.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
      "phpunit/phpunit": "^9.5",
      "phpmd/phpmd": "@stable",
      "friendsofphp/php-cs-fixer": "^3.0"
    },
    "require": {
      "php": "^7.4|^8.0",
      "guzzlehttp/guzzle": "^7.3",
      "ext-json": "*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}

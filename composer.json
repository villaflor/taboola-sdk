{
    "name": "villaflor/taboola-sdk",
    "description": "This package contains the open source PHP SDK that allows you to access the Taboola Platform from you PHP application.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Villaflor\\TaboolaSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Villaflor\\TaboolaSDK\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Mark Anthony Villaflor",
            "email": "villaflor@live.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3",
        "villaflor/connection": "^1.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
